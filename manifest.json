{
    "manifest_version": 2,
    "name": "FactChecker",
    "version": "69.69",

    "content scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_icon": "logo_png",
      "default_popup": "popup.html",
      "default_title": "The popup comes here."
    }
  }